{
  "name": "vite-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w server/src -x tsx server/src/index.ts",
    "start": "NODE_ENV=production node dist/server/src/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "test:unit": "jest --testPathPattern=server/tests/unit-tests --config=./jest.config.js",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "build": "NODE_ENV=production vite build && NODE_ENV=production tsc -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "eslint ./client",
    "seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.14.1",
    "@prisma/client": "6.4.1",
    "@sesamiapp/app-message": "^0.1.0",
    "@tanstack/react-query": "^5.55.0",
    "@types/prettier": "^3.0.0",
    "antd": "^5.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "mongodb": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.5.0",
    "prisma": "6.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "sass-embedded": "^1.79.3",
    "styled-components": "^6.1.13",
    "vite-express": "*",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@stylistic/eslint-plugin-ts": "^2.7.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "msw": "^2.4.11",
    "nodemon": "^3.1.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.5.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
